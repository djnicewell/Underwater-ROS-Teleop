<?xml version="1.0"?>
<launch>
	<node pkg="rosserial_python" type="serial_node.py" name="motorbox1">
		<param name="port" value="/dev/ttyUSB0"/>
	</node>

	<node pkg="rosserial_python" type="serial_node.py" name="motorbox2">
		<param name="port" value="/dev/ttyUSB1"/>
	</node>

	<node pkg="rosserial_python" type="serial_node.py" name="motorbox3">
		<param name="port" value="/dev/ttyUSB2"/>
	</node>

	<node pkg="rosserial_python" type="serial_node.py" name="centermotor">
		<param name="port" value="/dev/ttyUSB3"/>
	</node>
</launch>